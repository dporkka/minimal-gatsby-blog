{
  "name": "minimal-gatsby-blog",
  "private": true,
  "description": "A Minimalist Gatsby Blog Template",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "start": "yarn clear:babel-cache && next",
    "build": "yarn clear:babel-cache next build && next export",
    "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*"
  },
  "dependencies": {
    "@types/react": "^15.0.0",
    "bindings": "^1.5.0",
    "core-js": "3",
    "gatsby": "^2.19.5",
    "gatsby-cli": "^2.12.51",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-dark-mode": "^1.1.2",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.6.14",
    "gatsby-remark-images": "^3.1.42",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.3.13",
    "node": "^14.4.0",
    "node-sass": "^4.14.1",
    "prop-types": "^15.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-ga": "^3.0.0",
    "react-helmet": "^5.2.1",
    "react-mailchimp-subscribe": "^2.1.0",
    "react-refresh": "^0.8.3",
    "react-refresh-webpack-plugin": "^0.1.0",
    "sass": "^1.26.9",
    "scandir": "^0.1.2",
    "tsutils": "3.17.1",
    "typescript": "^3.8.3",
    "webpack": "^4.44.0"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dporkka/minimal-gatsby-blog"
  },
  "bugs": {
    "url": "https://github.com/dporkka/minimal-gatsby-blog/issues"
  }
}
